[
  {
    "environment": "dev",
    "description": "development environment for developer",
    "authenticationType": "api-key",
    "authenticationSecret": "My-Super-Secret",
    "applications": [
      {
        "name": "users-api",
        "description": "secrets for users api",
        "secrets": [
          {
            "key": "elasticsearch.port",
            "value": "9200",
            "description": "default elasticsearch port"
          },
          {
            "key": "elasticsearch.url",
            "value": "https://xxxx",
            "description": "elasticsearch url"
          },
          {
            "key": "elasticsearch.user",
            "value": "admin",
            "description": "elasticsearch user access"
          },
          {
            "key": "elasticsearch.password",
            "value": "password",
            "description": "elasticsearch user password"
          }
        ]
      }
    ]
  }
]